{
  "root": true,
  "extends": "standard",
  "parser": "@babel/eslint-parser",
  "plugins": [
    "simple-import-sort"
  ],
  "rules": {
    "simple-import-sort/imports": "error",
    "simple-import-sort/exports": "error"
  },
  "env": {
    "node": true
  },
  "ignorePatterns": [
    "**/node_modules/*"
  ]
}
