{
  "name": "ava-shared-workers-experiment",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "ava",
    "debug": "DEBUG=\"browsertest:*\" DEBUG_COLORS=yes SHOW_BROWSER=1 ava --verbose --serial",
    "test": "eslint \"**/*.js\"",
    "format": "prettier-eslint --write \"$PWD/**/*.js\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "ava": "^3.15.0",
    "chrome-location": "^1.2.1",
    "debug": "^4.3.1",
    "expressions-js": "^0.1.12",
    "fastify": "^3.17.0",
    "fastify-cors": "^6.0.1",
    "firefox-location": "^1.0.2",
    "globby": "^11.0.3",
    "js-yaml": "^4.1.0",
    "micromatch": "^4.0.4",
    "mime-types": "^2.1.30",
    "ngrok": "^4.0.1",
    "puppeteer-core": "^10.0.0",
    "rxjs": "^7.1.0"
  },
  "devDependencies": {
    "eslint": "^7.27.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "prettier-eslint-cli": "^5.0.1"
  },
  "lint-staged": {
    "*.js": [
      "prettier-eslint --write"
    ]
  }
